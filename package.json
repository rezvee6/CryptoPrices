{
  "name": "bitcoinprice",
  "version": "1.0.0",
  "description": "bitcoin price tracker",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "build",
    "package-win": "electron-packager ./bin --platform=win32 --arch=ia32 --out=./dist --overwrite=true"
  },
  "build": {
    "appId": "BitcoinPrice",
    
    "win":{
      "target": "nsis",
      "icon": "img/bitcoin.ico"
    }
  },
  "author": "Reezvee Sikder",
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.1.1",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1"
  }
}
